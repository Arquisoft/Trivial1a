# Routes
# This file defines all application routes (Higher priority routes first)
# ~~~~

# Home page
GET     /                           controllers.Application.index()
GET  	/public/*file		controllers.Assets.at(path="/public", file)
GET     /Registro                           controllers.Application.index2()
GET    /prejuego                       controllers.Application.index3()
GET    /ayuda                          controllers.Application.index4()
GET    /estadisticas                         controllers.Application.index5()
# Map static resources from the /public folder to the /assets URL path
#GET     /tasks                  controllers.Application.tasks()
#POST    /tasks                  controllers.Application.newTask()
#POST    /tasks/:id/delete       controllers.Application.deleteTask(id: String)
GET		/users					controllers.Application.users()
